<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".scanktp">

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/panahback"
        android:id="@+id/panahback1"
        android:layout_marginTop="55dp"
        android:layout_marginLeft="40dp"
        android:onClick="@drawable/panahback"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/teks_scantutupbotol"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="60dp"
        android:id="@+id/teksscantutup"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/img_fotoprofil"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="150dp"
        android:id="@+id/fotoprofil"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/teks_totaltutupbotol"
        android:layout_below="@+id/fotoprofil"
        android:layout_marginLeft="110dp"
        android:id="@+id/tekstotaltutupbotol"
        android:layout_marginTop="10dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/teks_scanktpuntukmelanjutkan"
        android:layout_below="@+id/tekstotaltutupbotol"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="60dp"
        android:id="@+id/tekssetiapkali"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/img_scanktp"
        android:layout_below="@+id/tekssetiapkali"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:id="@+id/scanktp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/img_ktp"
        android:layout_below="@+id/tekssetiapkali"
        android:layout_marginTop="46dp"
        android:layout_centerHorizontal="true"/>


    <Button
        android:background="@drawable/button_shape"
        app:backgroundTint="@color/biru"
        android:id="@+id/btnkliksavektp"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="90dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/teks_save"
        android:layout_marginTop="610dp"
        android:layout_marginLeft="170dp"/>

    <Button
        android:background="@drawable/button_shape_register"
        app:backgroundTint="@color/biru"
        android:id="@+id/btnklikcancel5"
        android:layout_width="300dp"
        android:layout_height="70dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="10dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/teks_cancel"
        android:layout_marginTop="690dp"
        android:layout_marginLeft="170dp"/>




</RelativeLayout>